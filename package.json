{
  "name": "hitem3d-square",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "type-check": "tsc --noEmit",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --runInBand",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .ts,.tsx,.js,.cjs --max-warnings=0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "openapi:validate": "swagger-cli validate docs/spec/api.openapi.yaml",
    "ci": "npm run type-check && npm run lint && npm run format:check && npm run openapi:validate && npm run test && npm run build"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@supabase/supabase-js": "2.45.4"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "4.0.4",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "@playwright/test": "1.48.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.30",
    "@types/node": "20.11.30",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "9.1.0",
    "nock": "13.5.2",
    "undici": "6.19.8",
    "prettier": "3.3.3",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "typescript": "5.6.2"
  }
}
